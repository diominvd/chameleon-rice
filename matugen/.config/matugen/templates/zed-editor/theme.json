{
  "name": "Matugen",
  "author": "diominvd",
  "themes": [
    {
      "name": "Matugen Dynamic",
      "appearance": "dark",
      "style": {
        "background": "{{colors.surface.default.hex}}",
        "background.appearance": "blurred",
        "canvas.background": "{{colors.surface.default.hex}}",
        "status_bar.background": "{{colors.surface_container.default.hex}}",
        "title_bar.background": "{{colors.surface.default.hex}}",
        "toolbar.background": "{{colors.surface.default.hex}}",
        "tab_bar.background": "{{colors.surface_dim.default.hex}}",
        "tab.active_background": "{{colors.surface_container_high.default.hex}}",
        "tab.inactive_background": "{{colors.surface_dim.default.hex}}",
        "tab.active_foreground": "{{colors.on_surface.default.hex}}",
        "panel.background": "{{colors.surface.default.hex}}",
        "editor.background": "{{colors.surface.default.hex}}",
        "editor.foreground": "{{colors.on_surface.default.hex}}",
        "editor.line_number": "{{colors.outline.default.hex}}",
        "editor.active_line_number": "{{colors.primary.default.hex}}",
        "editor.gutter.background": "{{colors.surface.default.hex}}",
        "editor.selection.background": "{{colors.primary_container.default.hex}}",
        "editor.active_line.background": "{{colors.surface_container.default.hex}}",
        "terminal.background": "{{colors.surface.default.hex}}",
        "terminal.ansi.black": "{{colors.surface_bright.default.hex}}",
        "terminal.ansi.red": "{{colors.error.default.hex}}",
        "terminal.ansi.green": "{{colors.primary.default.hex}}",
        "terminal.ansi.yellow": "{{colors.tertiary.default.hex}}",
        "terminal.ansi.blue": "{{colors.secondary.default.hex}}",
        "terminal.ansi.magenta": "{{colors.primary_container.default.hex}}",
        "terminal.ansi.cyan": "{{colors.secondary_container.default.hex}}",
        "terminal.ansi.white": "{{colors.on_surface.default.hex}}",
        "border": "{{colors.outline_variant.default.hex}}",
        "element.active": "{{colors.primary.default.hex}}",
        "element.selected": "{{colors.secondary_container.default.hex}}",
        "scrollbar.thumb.background": "{{colors.outline.default.hex}}66",
        "syntax": {
          "comment": {
            "color": "{{colors.outline.default.hex}}",
            "font_style": "italic"
          },
          "string": { "color": "{{colors.tertiary.default.hex}}" },
          "keyword": {
            "color": "{{colors.primary.default.hex}}",
            "font_weight": 700
          },
          "function": { "color": "{{colors.secondary.default.hex}}" },
          "type": { "color": "{{colors.primary_container.default.hex}}" },
          "variable": { "color": "{{colors.on_surface.default.hex}}" },
          "variable.special": { "color": "{{colors.secondary.default.hex}}" },
          "variant": { "color": "{{colors.primary.default.hex}}" },
          "label": { "color": "{{colors.secondary.default.hex}}" },
          "enum": { "color": "{{colors.primary.default.hex}}" },
          "property": { "color": "{{colors.secondary.default.hex}}" },
          "constant": { "color": "{{colors.tertiary.default.hex}}" },
          "boolean": { "color": "{{colors.tertiary.default.hex}}" },
          "number": { "color": "{{colors.tertiary.default.hex}}" },
          "operator": { "color": "{{colors.outline.default.hex}}" },
          "punctuation": { "color": "{{colors.outline.default.hex}}" },
          "attribute": { "color": "{{colors.primary.default.hex}}" },
          "tag": { "color": "{{colors.primary.default.hex}}" },
          "link_text": {
            "color": "{{colors.secondary.default.hex}}",
            "text_decoration": "underline"
          },
          "link_uri": { "color": "{{colors.tertiary.default.hex}}" }
        }
      }
    }
  ]
}
